{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "EACHLABS_API_KEY": "@eachlabs_api_key",
    "keyID": "@b2_key_id",
    "keyName": "@b2_key_name",
    "applicationKey": "@b2_app_key"
  }
}

